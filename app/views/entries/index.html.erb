<% @entries.each do |entry| %>
   		<div>
			<h2><%= link_to(entry.title, entry_path(entry)) %></h2>
			<%= button_to 'Destroy', entry, method: :delete, :onclick => "return confirm('Are you sure you want to delete this entry?')" ,:class=>"destroy_button"%>

			</div>
   		<% end %>

<%= link_to("New entry", new_entry_path) %>